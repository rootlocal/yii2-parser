{
  "name": "rootlocal/yii2-parser",
  "description": "extension site page",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "parser",
    "HtmlPurifier",
    "Parsedown"
  ],
  "homepage": "https://github.com/rootlocal/yii2-parser",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Alexander Zakharov",
      "email": "sys@eml.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4|^8.0|^8.1",
    "yiisoft/yii2": "~2.0.0",
    "yii2mod/yii2-markdown": "~1.1",
    "cebe/markdown": "~1.2.1",
    "whichbrowser/parser": "~2.1.7",
    "erusev/parsedown": "^1.7.4",
    "xemlock/htmlpurifier-html5": "^0.1.11"
  },
  "require-dev": {
  },
  "autoload": {
    "psr-4": {
      "rootlocal\\parser\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "rootlocal\\parser\\Bootstrap"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}