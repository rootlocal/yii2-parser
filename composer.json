{
  "name": "rootlocal/yii2-parser",
  "description": "extension site page",
  "type": "yii2-extension",
  "keywords": [
    "yii2",
    "extension",
    "parser",
    "HtmlPurifier",
    "Parsedown"
  ],
  "homepage": "http://rootlocal.ru",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Alexander Zakharov",
      "email": "sys@eml.ru",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "~2.0.0",
    "yii2mod/yii2-markdown": "*",
    "cebe/markdown": "~1.1.0",
    "whichbrowser/parser": "^2.0",
    "erusev/parsedown": "^1.7",
    "xemlock/htmlpurifier-html5": "^0.1.11"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "~2.0",
    "codeclimate/php-test-reporter": "^0.4.4"
  },
  "autoload": {
    "psr-4": {
      "rootlocal\\parser\\": "src/"
    }
  },
  "extra": {
    "bootstrap": "rootlocal\\parser\\Bootstrap"
  },
  "asset-installer-paths": {
    "npm-asset-library": "vendor/npm",
    "bower-asset-library": "vendor/bower"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}